gg.toast("ℹOnline Script🛡")

cevap = gg.alert("ℹScripti açmak için internetinizin olması gerekli. Devam etmek ister misin?🔰","✅Evet","❎Hayır")

if cevap == nil then
os.exit()
end
if cevap == 1 then
alpha = load(gg.makeRequest('https://raw.githubusercontent.com/abgteknisyen67/alphaonlinescript/main/script1').content)
pcall(alpha)
end
if cevap == 2 then
gg.toast("ℹScript kapatıldı")
os.exit()
end
